<Window x:Class="ParkingCourseProject.Views.EnterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParkingCourseProject.Views"
        mc:Ignorable="d"
        Title="EnterWindow" WindowStyle="None" Height="720" Width="1280" ResizeMode="NoResize" WindowState="Maximized">
    <Grid>
        <Grid Background="#3c3c3c" Height="40" VerticalAlignment="Top" Width="Auto" Grid.ColumnSpan="2"/>
        <Button Background="White" Content="X" HorizontalAlignment="Right" VerticalAlignment="Top" Width="23" BorderBrush="Black" Margin="0,7,6,0" Height="25" Padding="0,0,0,0" Foreground="Black" Grid.ColumnSpan="2" Click="Button_Click_1"/>
        <Button Background="White" Content="_" VerticalContentAlignment="Top" HorizontalAlignment="Right" VerticalAlignment="Top" Width="23" BorderBrush="Black" Margin="0,7,35,0" Height="25" Padding="0,0,0,0" Foreground="Black" Click="Button_Click"/>
        <Frame Name="Mainframe" NavigationUIVisibility="Hidden" Margin="0,40,0,0" Grid.ColumnSpan="2">

        </Frame>
    </Grid>
</Window>
